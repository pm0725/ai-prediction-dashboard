{
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://<YOUR_BACKEND_URL>/api/:path*"
        },
        {
            "source": "/ws/:path*",
            "destination": "https://<YOUR_BACKEND_URL>/ws/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}